#include <stdio.h>

int main() {
    int A[] = {5, 0, 10, 6};
    int B[] = {1, 2, 4};

    int m = sizeof(A) / sizeof(A[0]);
    int n = sizeof(B) / sizeof(B[0]);

    int prod[m + n - 1]; 


    for (int i = 0; i < m + n - 1; i++)
        prod[i] = 0;

  
    for (int i = 0; i < m; i++) {
        for (int j = 0; j < n; j++) {
            prod[i + j] += A[i] * B[j];
        }
    }

    // Print result
    printf("Product[] = ");
    for (int i = 0; i < m + n - 1; i++)
        printf("%d ", prod[i]);
    printf("\n");

    return 0;
}
