#include <stdio.h>

int main() {
    int n, i, j, isIdentity = 1;

    printf("Enter order of square matrix: ");
    scanf("%d", &n);

    int mat[n][n];

    printf("Enter the elements of the matrix:\n");
    for (i = 0; i < n; i++) {
        for (j = 0; j < n; j++) {
            scanf("%d", &mat[i][j]);
        }
    }

    for (i = 0; i < n; i++) {
        for (j = 0; j < n; j++) {
            if (i == j && mat[i][j] != 1) { 
                isIdentity = 0;
                break;
            }
            else if (i != j && mat[i][j] != 0) { 
                isIdentity = 0;
                break;
            }
        }
        if (!isIdentity) break;
    }

    if (isIdentity)
        printf("True\n");
    else
        printf("False\n");

    return 0;
}
