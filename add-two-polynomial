#include <stdio.h>

int main() {
    int A[] = {5, 0, 10, 6};
    int B[] = {1, 2, 4};
    int m = sizeof(A) / sizeof(A[0]);
    int n = sizeof(B) / sizeof(B[0]);

    int size = (m > n) ? m : n;
    int sum[size];

    for (int i = 0; i < size; i++)
        sum[i] = 0;

    for (int i = 0; i < m; i++)
        sum[i] += A[i];

    
    for (int i = 0; i < n; i++)
        sum[i] += B[i];

    printf("Sum[] = ");
    for (int i = 0; i < size; i++)
        printf("%d ", sum[i]);
    printf("\n");

    return 0;
}
